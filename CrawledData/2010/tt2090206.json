                "2": {
                    "title": "Tribeca Loft",
                    "kind": "episode",
                    "episode of": {
                        "title": "Million Dollar Contractor (TV Series 2010–2015)",
                        "kind": "tv series"
                    },
                    "season": 4,
                    "episode": 2,
                    "original air date": "11 Oct. 2014",
                    "year": "2014",
                    "plot": "\n    Fanuka teams up with an up and coming designer to give a downtown loft apartment a major style upgrade. The bold design pushes Fanuka's team to the limit, but in the end they create a space that's as functional as it is beautiful.    "
                },
                "3": {
                    "title": "Guest Suite Gut Reno",
