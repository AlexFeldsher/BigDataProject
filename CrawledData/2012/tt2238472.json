                    "kind": "episode",
                    "episode of": {
                        "title": "Kamen Rider Wizard (TV Series 2012– )",
                        "kind": "tv series"
                    },
                    "season": 1,
                    "episode": 39,
                    "original air date": "9 Jun. 2013",
                    "year": "2013",
